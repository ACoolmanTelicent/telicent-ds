{
  "name": "@telicent-oss/ds",
  "description": "A component library for creating Telicent apps.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telicent-oss/telicent-ds.git"
  },
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/base": "5.0.0-beta.38",
    "@mui/material": "^5.15.10",
    "@storybook/manager-api": "7.6.17",
    "@storybook/theming": "7.6.17",
    "@tanstack/react-query": "^5.18.0",
    "@telicent-oss/ontologyservice": "^0.0.1",
    "classnames": "^2.3.1",
    "d3": "^7.8.2",
    "lodash": "^4.17.21",
    "maplibre-gl": "^4.0.0",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "7.1.0",
    "typescript": "*",
    "zod": "^3.21.4"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "scripts": {
    "lint": "eslint --resolve-plugins-relative-to src",
    "clean": "rimraf dist storybook-static",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "yarn prestart && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-docs": "storybook dev --docs --no-manager-cache",
    "build-storybook-docs": "storybook build --docs",
    "test:ci": "yarn test --ci --json --outputFile=\"results.json\" --watchAll=false",
    "chromatic": "chromatic --exit-zero-on-changes",
    "css": "npx tailwindcss -i ./src/index.css -o ./src/main.css",
    "prestart": "yarn run css",
    "prebuild": "yarn clean"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.18.0",
    "@telicent-oss/ontologyservice": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.7",
    "@storybook/addon-docs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@vitejs/plugin-react": "^4.2.0",
    "chromatic": "^6.15.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "react-scripts": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "rollup-plugin-copy": "^3.5.0",
    "storybook": "7.6.7",
    "storybook-dark-mode": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.2",
    "vite-plugin-dts": "^3.6.3"
  },
  "publishConfig": {
    "registry": "https://www.npmjs.com/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ds.umd.cjs",
  "module": "./dist/ds.js",
  "types": "./dist/export.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ds.js",
      "require": "./dist/ds.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "style": "./dist/style.css"
    },
    "./dist/fontawesome.css": {
      "import": "./dist/fontawesome.css",
      "require": "./dist/fontawesome.css",
      "style": "./dist/fontawesome.css"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^d3$": "<rootDir>/node_modules/d3/dist/d3.min.js"
    },
    "transformIgnorePatterns": [
      "node_modules\\/(?!(@telicent-oss/ontologyservice))"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
